GOOGLE SCRIPT - COPY VALIDATION FROM SHEET SOURCE TO SHEET DESTINATION

function VMK_COPY_VALIDATION_TO_RANGE(SPREADSHEET_ID, SHEET_SOURCE_NAME, SHEET_DESTINATION_NAME, RANGE_SOURCE, RANGE_DESTINATION)
{
	var SS, SHEET_SOURCE, SHEET_DESTINATION;
	SS = SpreadsheetApp.openById(SPREADSHEET_ID);
	SHEET_SOURCE = SS.getSheetByName(SHEET_SOURCE_NAME);
	RANGE_SOURCE = SHEET_SOURCE.getRange(RANGE_SOURCE);
	SHEET_DESTINATION = SS.getSheetByName(SHEET_DESTINATION_NAME);
	RANGE_DESTINATION = SHEET_DESTINATION.getRange(RANGE_DESTINATION);
	var RULES_SOURCE = RANGE_SOURCE.getDataValidations();
	RANGE_DESTINATION.setDataValidations(RULES_SOURCE);
}
