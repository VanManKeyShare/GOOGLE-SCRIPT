
GOOGLE SCRIPT - COPY FORMAT FROM RANGE SOURCE TO RANGE DESTINATION

function VMK_COPY_FORMAT_TO_RANGE(SPREADSHEET_ID, SHEET_SOURCE_NAME, RANGE_SOURCE, SHEET_DESTINATION_NAME, COLUMN, COLUMN_END, ROW, ROW_END)
{
	var SS, SHEET_SOURCE, SHEET_DESTINATION;
	SS = SpreadsheetApp.openById(SPREADSHEET_ID);
	SHEET_SOURCE = SS.getSheetByName(SHEET_SOURCE_NAME);
	SHEET_DESTINATION = SS.getSheetByName(SHEET_DESTINATION_NAME);
	RANGE_SOURCE = SHEET_SOURCE.getRange(RANGE_SOURCE);
	RANGE_SOURCE.copyFormatToRange(SHEET_DESTINATION, COLUMN, COLUMN_END, ROW, ROW_END);
}
